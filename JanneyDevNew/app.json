{
  "expo": {
    "slug": "janney-online-access",
    "version": "4.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "orientation": "portrait",
    "scheme": "janneyapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "buildVersion": 1,
      "eas": {
        "projectId": "1dfea2f8-c389-4cda-9f90-7515bb9ca474"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 30000,
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/1dfea2f8-c389-4cda-9f90-7515bb9ca474"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app will optionally use Face ID or Touch ID to save login",
        "NSCameraUsageDescription": "This application uses the camera to take pictures of Checks and documents",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to be able to download files to your phone",
        "NSPhotoLibraryUsageDescription": "This app needs access to be able to download files to your phone",
        "ITSAppUsesNonExemptEncryption": false,
        "iCloudContainerEnvironment": "Production"
      }
    },
    "android": {
      "permissions": [
        "INTERNET",
        "SYSTEM_ALERT_WINDOW",
        "DOWNLOAD_WITHOUT_NOTIFICATION",
        "CAMERA",
        "MANAGE_DOCUMENTS",
        "USE_FINGERPRINT",
        "VIBRATE",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE",
        "com.htc.launcher.permission.READ_SETTINGS",
        "com.htc.launcher.permission.UPDATE_SHORTCUT",
        "com.majeur.launcher.permission.UPDATE_BADGE",
        "com.sec.android.provider.badge.permission.READ",
        "com.sec.android.provider.badge.permission.WRITE",
        "com.sonyericsson.home.permission.BROADCAST_BADGE"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "translucent": false
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 34
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
